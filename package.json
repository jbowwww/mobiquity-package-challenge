{
  "name": "mobiquity-package-challenge",
  "version": "1.0.0",
  "description": "A node.js module for optimising package contents based on value, weight, and maximum package weight.",
  "main": "dist/index.js",
  "types" : "dist/index.d.ts",
  "type": "module",
  "devDependencies": {
    "@types/jest": "^29.2.2",
    "@types/node": "^18.11.9",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  },
  "scripts": {
    "start": "npm run build:live",
    "build": "tsc -p .",
    "build:live": "nodemon --watch 'src/**/*.ts' --exec \"ts-node\" src/index.ts",
    "clean": "rm -rfv ./dist/*", 
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jbowwww/mobiquity-package-challenge.git"
  },
